var nsp=io.of('/everything-moto')
nsp.on('connection', function (socket){
  console.log("a user connected to moto")
})
nsp.emit ('hi')

export function motoMessage(message) {
  var socket=io('/everything-moto')
  socket.emit('newMotoMessage', message)
}

